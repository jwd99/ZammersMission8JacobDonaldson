@{
}
@model IEnumerable<string>

    <div class="d-grid gap-4">
        <a asp-controller="Home" asp-action="Index" class="btn btn-primary">Home</a>
    </div>

    @foreach(string text in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-bookCategory="@text" class="btn btn-secondary">@text</a>
    }